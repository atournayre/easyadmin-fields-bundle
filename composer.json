{
    "description": "EasyAdmin fields bundle",
    "name": "atournayre/easyadmin-fields-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "atournayre/helpers": "^1.0",
        "easycorp/easyadmin-bundle": "^4.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\EasyAdminFieldsBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\EasyAdminFieldsBundle\\Tests\\": "tests/" }
    }
}
